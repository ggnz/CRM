            <input type="hidden" asp-for="Clinica.IdClinica" />
            <div class="form-group">
                <label asp-for="Clinica.Nombre" class="control-label"></label>
                <input asp-for="Clinica.Nombre" class="form-control" />
                <span asp-validation-for="Clinica.Nombre" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clinica.Descripcion" class="control-label"></label>
                <input asp-for="Clinica.Descripcion" class="form-control" />
                <span asp-validation-for="Clinica.Descripcion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clinica.IdRepresentante" class="control-label"></label>
                <input asp-for="Clinica.IdRepresentante" class="form-control" />
                <span asp-validation-for="Clinica.IdRepresentante" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clinica.Direccion" class="control-label"></label>
                <input asp-for="Clinica.Direccion" class="form-control" />
                <span asp-validation-for="Clinica.Direccion" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clinica.Categoria" class="control-label"></label>
