            </div>
            <div class="form-group">
                <label asp-for="Clinica.Ciudad" class="control-label"></label>
                <input asp-for="Clinica.Ciudad" class="form-control" />
                <span asp-validation-for="Clinica.Ciudad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clinica.Telefono" class="control-label"></label>
                <input asp-for="Clinica.Telefono" class="form-control" />
                <span asp-validation-for="Clinica.Telefono" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Clinica.Email" class="control-label"></label>
                <input asp-for="Clinica.Email" class="form-control" />
                <span asp-validation-for="Clinica.Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>
