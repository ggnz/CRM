@page
@model OpenSaludSecurity.Pages.Clinicas.EditModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Edit</title>
</head>
<body>

<h4>Clinica</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
