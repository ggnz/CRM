@page
@model OpenSaludSecurity.Controller.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Medico[0].NombreMedico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Medico[0].Ap1Medico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Medico[0].Ap2Medico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Medico[0].Categoria)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Medico) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMedico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ap1Medico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ap2Medico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Categoria)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.IdMedico">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.IdMedico">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.IdMedico">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
